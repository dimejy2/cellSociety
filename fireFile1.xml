		<row values="110000000000000"></row>
		<row values="000000200011111"></row>
		<row values="000000000011211"></row>
		<row values="011000200011211"></row>
		<row values="010000000011111"></row>
		<row values="000010220200101"></row>
		<row values="000110000010101"></row>
		<row values="000000011111011"></row>
		<row values="000000200011001"></row>
		<row values="010000200011111"></row>
		<row values="000010000000101"></row>
		<row values="000120000210101"></row>
