<?xml version="1.0" encoding="UTF-8"?>
<!-- Type is the type of simulation -->
<cellularautomata type = "GameofLife">
	<properties>
		<!-- Parameters for gamemode -->
		<property name="resources" value="10"></property>
		<property name ="numCellStates" value ="2"></property>
		<colors>
			<color state="0" color = "white"></color>
			<color state="1" color = "black"></color>
		</colors>
	</properties>
	<grid row="10" column="10" gridType="HexagonBoard">
	<!-- DEFAULT GRID -->
	<!-- Each digit represents a cell with its value being
	its initial state. -->
		<row values="1100000000"></row>
		<row values="1100000000"></row>
		<row values="0000000000"></row>
		<row values="0000000000"></row>
		<row values="0110000000"></row>
		<row values="0100000000"></row>
		<row values="0000100000"></row>
		<row values="0001100000"></row>
		<row values="0000000111"></row>
		<row values="0000000000"></row>
	</grid>
</cellularautomata>