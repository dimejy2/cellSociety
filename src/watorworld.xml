<?xml version="1.0" encoding="UTF-8"?>
<!-- Type is the type of simulation -->
<cellularautomata type = "WaTorWorld">
	<properties>
		<!-- Parameters for gamemode -->
		<property name="resources" value="10"></property>
		<property name ="numCellStates" value ="3"></property>
		<property name ="probability" value ="0.7"></property>
		<colors>
			<color state="0" color = "blue"></color>
			<color state="1" color = "green"></color>
			<color state="2" color = "orange"></color>
		</colors>
	</properties>
	<grid xValue="5" yValue="9" gridType="HexagonBoard">
	<!-- DEFAULT GRID -->
	<!-- Each digit represents a cell with its value being
	its initial state. -->
		<row values="210210210"></row>
		<row values="211210210"></row>
		<row values="211210210"></row>
		<row values="211210210"></row>
		<row values="211210210"></row>
	</grid>
</cellularautomata>