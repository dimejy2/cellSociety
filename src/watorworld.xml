<?xml version="1.0" encoding="UTF-8"?>
<!-- Type is the type of simulation -->
<cellularautomata type = "WaTorWorld">
	<properties>
		<!-- Parameters for gamemode -->
		<property name ="numCellStates" value ="3"></property>
		<property name ="cellIncrement" value ="4"></property>
		<property name ="cellDecrement" value ="1"></property>
		<property name ="breedTime" value ="7"></property>
		<property name ="cellResources" value="6"></property>
		
		<colors>
			<color state="0" color = "blue"></color>
			<color state="1" color = "green"></color>
			<color state="2" color = "orange"></color>
		</colors>
	</properties>
	<grid row="10" column="10" gridType="SquareBoard">
	<!-- DEFAULT GRID -->
	<!-- Each digit represents a cell with its value being
	its initial state. -->
		<row values="1010122021"></row>
		<row values="1112112112"></row>
		<row values="0121121202"></row>
		<row values="0021210121"></row>
		<row values="0121111220"></row>
		<row values="0110221212"></row>
		<row values="0212121112"></row>
		<row values="1212121221"></row>
		<row values="2121212121"></row>
		<row values="1212112111"></row>

	</grid>
</cellularautomata>