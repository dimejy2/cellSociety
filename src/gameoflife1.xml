<?xml version="1.0" encoding="UTF-8"?>
<!-- Type is the type of simulation -->
<cellularautomata type="GameofLife">
	<properties>
		<!-- Parameters for gamemode -->
		<property name="resources" value="10"></property>
		<property name="numCellStates" value="2"></property>
		<colors>
			<color state="0" color="white"></color>
			<color state="1" color="black"></color>
		</colors>
	</properties>
	<grid xValue="15" yValue="15" gridType="HexagonBoard">
		<!-- DEFAULT GRID -->
		<!-- Each digit represents a cell with its value being its initial state. -->
		<row values="110000000011111"></row>
		<row values="110000000000000"></row>
		<row values="000000000011111"></row>
		<row values="000000000011111"></row>
		<row values="011000000011111"></row>
		<row values="010000000011111"></row>
		<row values="000010000000101"></row>
		<row values="000110000010101"></row>
		<row values="000000011111011"></row>
		<row values="000000000011001"></row>
		<row values="010000000011111"></row>
		<row values="000010000000101"></row>
		<row values="000110000010101"></row>
		<row values="000000011111011"></row>
		<row values="000000000011001"></row>
	</grid>
</cellularautomata>